<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jaeho.atgg.mapper.MatchMapper">

	<insert id="insertMatchRef">
		insert into MatchRef 
		values(#{gameId}, #{gameCreation}, #{gameDuration}, #{queueId})
	</insert>
	
	<insert id="insertTeams">
		insert into Teams 
		values(#{gameId}, #{towerKills}, #{dragonKills}, #{baronKills}, #{teamId}, #{win})
	</insert>
	
	<insert id="insertParticipant">
		insert into Participant 
		values(#{summoner}, #{gameId}, #{participantId}, #{championId}, #{teamId}, #{spell1Id}, #{spell2Id})
	</insert>
	
	<insert id="insertParticipantStats">
		insert into ParticipantStats 
		values(
		#{gameId}, #{participantId}, #{totalDamageDealtToChampions},
		#{item0}, #{item1}, #{item2}, #{item3}, #{item4}, #{item5},
		#{largestMultiKill}, #{goldEarned}, #{wardsKilled}, #{wardsPlaced}, 
		#{totalMinionsKilled}, #{visionWardsBoughtInGame},
		#{kills},#{deaths}, #{assists},
		#{neutralMinionsKilled},#{perk0},#{perk1},#{perk2},#{perk3},#{perk4},#{perk5},
		#{perkPrimaryStyle},#{perkSubStyle},#{statPerk0},#{statPerk1},#{statPerk2}, #{win})
	</insert>
	
	<insert id="insertTimeline">
		insert into Timeline 
		values(#{gameId}, #{participantId}, #{role}, #{lane});
	</insert>

</mapper>